{
"manifest_version": 3,
"name": "Tabnabber",
"description": "This extension detects possible tabnabbing attacks",
"version": "1.0",
"action": {
    "default_icon": {"16":"redDot.png"}
},
"background": { 
    "service_worker": "background.js" 
},
"permissions": [
    "tabs",
    "storage",
    "offscreen",
    "scripting"
],
"host_permissions": [
    "<all_urls>"
]
}